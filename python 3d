print("enter elements of tuple")

l = raw_input().split()
t=tuple(l)
print(t)

x =raw_input('Enter a word to be searched in tuple: ')

if x in t :
	print('Element {} found '.format(x))
else:
	print("Elment {} not found".format(x))

